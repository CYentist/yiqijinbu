<table class="table table-boldered">
  <% @courses.each do |course| %>
  <tr>

    <td>
      <%= link_to(course.title, admin_course_path(course)) %>
    </td>
    <td>
      <%= link_to("Edit", edit_admin_course_path(course)) %> |
      <%= link_to("Destroy", admin_course_path(course), :method => :delete, :data => { :confirm => "Are you sure?" }) %>
    </td>
    <td>
      <%= course.created_at %>
    </td>
  </tr>
  <% end %>
</table>
